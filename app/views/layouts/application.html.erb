<!DOCTYPE html>
<html>
<head>
  <title>Scraping</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
<div class='container'>
  <p class="notice"><%= notice %></p>
  <p class="alert"><%= alert %></p>
  <div class='navbar'>
    <% if user_signed_in? %>
      <a href='/user/<%=current_user.id%>'>Home</a> | <a href='/about'>About</a> | <%= link_to "Log out", destroy_user_session_path,:method=> :delete %>
    <% end %>
  </div>
  <%= yield %>
</div>
</body>
</html>
